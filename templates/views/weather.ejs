<body>
    <%- include("../partials/navbar") %>
    <div class="main" class="col-md-10 col-12 mx-auto">
        <div class="row">
         <div class="main-header" class="col-md-10 col-12 main_header_left">
            <div class="main-content">
            <form class="main_form">
                <input type="text" placeholder="Enter your city name" autocomplete="off" class="inpVal">
                <input type="submit" value="search" class="sub">
            </form>
            </div>
            <div class="main-section">
                <div class="upper-section">
                     <p id="present-day">Saturday</p>
                     <p id="present-date">26 JAN</p>
                </div>
                <div class="middle-section">
                     <p id="cityName">Get Output Here</p>
                <div class="lower-section data_hide">
                     <p id="temp"><span id="degree">0</span><sup>o</sup>C</p>
                     <p id="image"><i class="fa fa-cloud"> </i></p>
                </div>
            </div>

            </div>
         </div>
      </div>
    </div>  
      <%- include("../partials/footer") %>
